#/bin/sh

function print {
	echo "\\section{$1}"
	echo "\\label{$1}"
	pygmentize -l $2 -f latex -P encoding=utf-8 $1
	echo
}

python generate_header.py

cd ../../../

for F in `find dejumblefs -name "*.py" | sed -E "/\\.\\//s///" | egrep -v "(^build/|/build/|.svn|__init__)"`
do
	print $F python
done

